            <TextLine id="tr_1_tl_14" custom="readingOrder {index:13;}">
                <Coords points="162,1492 188,1492 214,1494 240,1494 266,1493 292,1493 318,1492 344,1490 370,1490 396,1490 396,1442 370,1442 344,1442 318,1444 292,1445 266,1445 240,1446 214,1446 188,1444 162,1444"/>
                <Baseline points="162,1476 188,1476 214,1478 240,1478 266,1477 292,1477 318,1476 344,1474 370,1474 396,1474"/>
